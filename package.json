{
  "name": "@oleoneto/microkit",
  "description": "Microservices toolkit",
  "version": "0.2.2",
  "author": "Leo Neto",
  "maintainers": [
    "Leo Neto <leo@ekletik.com>"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "microkit": "./bin/run"
  },
  "bugs": "https://github.com/oleoneto/microkit/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@octokit/rest": "^18.0.6",
    "@prisma/client": "~2.7.1",
    "@types/knex": "~0.16.1",
    "@types/mysql": "^2.15.15",
    "@types/pg": "~7.14.5",
    "@types/ws": "~7.2.6",
    "ari-client": "~2.2.0",
    "aws-sdk": "~2.757.0",
    "axios": "~0.20.0",
    "bitbucket": "^2.4.1",
    "cli-ux": "~5.5.0",
    "dotenv": "~8.2.0",
    "fluent-ffmpeg": "~2.1.2",
    "gitignore": "^0.6.0",
    "inquirer": "~7.3.3",
    "kafka-node": "~5.0.0",
    "knex": "~0.21.5",
    "license": "~1.0.3",
    "listr": "~0.14.3",
    "moment": "~2.28.0",
    "ncp": "~2.0.0",
    "pg": "~8.3.3",
    "pkg-install": "~1.0.0",
    "simple-git": "~2.21.0",
    "tslib": "^1",
    "ws": "~7.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@prisma/cli": "~2.7.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "mysql": "~2.18.1",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "src/templates"
  ],
  "homepage": "https://github.com/oleoneto/microkit",
  "keywords": [
    "oclif"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "microkit",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.ekletik.microkit"
    }
  },
  "repository": "oleoneto/microkit",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
